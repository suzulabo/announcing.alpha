{
  "name": "announcing-client",
  "private": true,
  "watch": {
    "copy-shared": {
      "patterns": [
        "../shared-ui/src",
        "../shared/src"
      ],
      "extensions": "ts,tsx,scss"
    }
  },
  "scripts": {
    "build": "rm -rf www && stencil build",
    "build.cap": "rm -rf cap && CAP_BUILD=y stencil build && cap sync && cp google-services.json android/app",
    "serve": "stencil build --dev --watch --serve --service-worker",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "copy-shared": "node copy-shared.js",
    "copy-shared.watch": "npm-watch copy-shared",
    "start": "npm run serve"
  },
  "devDependencies": {
    "@capacitor-community/http": "^1.0.0",
    "@capacitor/android": "^3.0.0",
    "@capacitor/app": "^1.0.0",
    "@capacitor/cli": "^3.0.0",
    "@capacitor/core": "^3.0.0",
    "@capacitor/local-notifications": "^1.0.0",
    "@capacitor/push-notifications": "^1.0.0",
    "@capacitor/share": "^1.0.0",
    "@capacitor/storage": "^1.0.0",
    "@ionic/core": "^5.6.8",
    "@rollup/plugin-replace": "^2.4.2",
    "@stencil/core": "^2.5.2",
    "@stencil/sass": "^1.4.1",
    "@stencil/store": "^1.4.1",
    "autolinker": "^3.14.3",
    "base-x": "^3.0.8",
    "bootstrap-icons": "^1.5.0",
    "date-fns": "^2.21.3",
    "destyle.css": "^2.0.2",
    "firebase": "^8.6.2",
    "fs-extra": "^10.0.0",
    "idb": "^6.1.1",
    "intersection-observer": "^0.12.0",
    "pinch-zoom-element": "^1.1.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tweetnacl": "^1.0.3",
    "type-fest": "^1.1.3",
    "workbox-build": "^4.3.1"
  }
}
