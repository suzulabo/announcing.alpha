{
  "name": "announcing-client",
  "private": true,
  "scripts": {
    "build": "rm -rf www && npm run copy-shared && stencil build",
    "build.cap": "rm -rf cap && npm run copy-shared && CAP_BUILD=y stencil build",
    "build.android": "npm run build.cap && cap sync android && cp google-services.json android/app",
    "build.ios": "npm run build.cap && cap sync ios && cp GoogleService-Info.plist ios/App/App && cp App.entitlements ios/App/App",
    "serve": "stencil build --dev --watch --serve --service-worker",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "copy-shared": "node copy-shared.js",
    "copy-shared.watch": "node copy-shared.js -w",
    "start": "npm run copy-shared && run-p copy-shared.watch serve"
  },
  "devDependencies": {
    "@capacitor-community/http": "^1.0.0",
    "@capacitor/android": "^3.1.1",
    "@capacitor/app": "^1.0.2",
    "@capacitor/cli": "^3.1.1",
    "@capacitor/core": "^3.1.1",
    "@capacitor/ios": "^3.1.1",
    "@capacitor/local-notifications": "^1.0.3",
    "@capacitor/push-notifications": "^1.0.3",
    "@capacitor/share": "^1.0.3",
    "@capacitor/storage": "^1.0.3",
    "@rollup/plugin-replace": "^2.4.2",
    "@stencil/core": "^2.6.0",
    "@stencil/sass": "^1.4.1",
    "@stencil/store": "^1.4.1",
    "autolinker": "^3.14.3",
    "base-x": "^3.0.8",
    "bootstrap-icons": "^1.5.0",
    "date-fns": "^2.22.1",
    "destyle.css": "^2.0.2",
    "firebase": "^9.0.0-beta.6",
    "idb": "^6.1.2",
    "intersection-observer": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "pinch-zoom-element": "^1.1.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sync-directory": "^2.2.22",
    "tweetnacl": "^1.0.3",
    "type-fest": "^1.2.2",
    "typescript": "^4.3.5",
    "workbox-build": "^4.3.1"
  }
}
